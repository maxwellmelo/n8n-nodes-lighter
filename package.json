{
	"name": "n8n-nodes-lighter",
	"version": "0.4.2",
	"description": "n8n community node for Lighter (zkLighter) - A high-performance perpetuals DEX with AI Agent tools",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"lighter",
		"zklighter",
		"dex",
		"perpetuals",
		"trading",
		"crypto",
		"defi",
		"ai-agent",
		"langchain",
		"ai-tools"
	],
	"license": "MIT",
	"homepage": "https://github.com/maxwellmelo/n8n-nodes-lighter",
	"author": {
		"name": "Maxwell Melo",
		"email": "maxwell.melo0@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/maxwellmelo/n8n-nodes-lighter.git"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@9.1.4",
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/LighterApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Lighter/Lighter.node.js",
			"dist/nodes/Lighter/LighterTrigger.node.js",
			"dist/nodes/Lighter/LighterAccountTool.node.js",
			"dist/nodes/Lighter/LighterMarketTool.node.js",
			"dist/nodes/Lighter/LighterTradingTool.node.js",
			"dist/nodes/Lighter/LighterPositionTool.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@types/ws": "^8.18.1",
		"@typescript-eslint/parser": "^7.2.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^3.2.5",
		"typescript": "~5.4.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"@langchain/core": "^0.3.0",
		"ws": "^8.16.0",
		"zod": "^3.23.0"
	}
}
