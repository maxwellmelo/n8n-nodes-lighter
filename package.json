{
	"name": "n8n-nodes-lighter",
	"version": "0.1.0",
	"description": "n8n community node for Lighter (zkLighter) - A high-performance perpetuals DEX",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"lighter",
		"zklighter",
		"dex",
		"perpetuals",
		"trading",
		"crypto",
		"defi"
	],
	"license": "MIT",
	"homepage": "https://github.com/maxwellwynter/n8n-nodes-lighter",
	"author": {
		"name": "Maxwell Wynter",
		"email": "maxwell@example.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/maxwellwynter/n8n-nodes-lighter.git"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@9.1.4",
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/LighterApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Lighter/Lighter.node.js",
			"dist/nodes/Lighter/LighterTrigger.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@types/ws": "^8.18.1",
		"@typescript-eslint/parser": "^7.2.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^3.2.5",
		"typescript": "~5.4.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"ws": "^8.16.0"
	}
}
